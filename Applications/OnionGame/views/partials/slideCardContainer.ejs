<link rel="stylesheet" href="css/slide.css">

<div class="slideCardContainer col-md-5">
    <!--Where The cards will be placed-->

    <div class="cardStack" id='cardStack'></div>
    <div class="buttons">
        <button id="onion">ONION</button>
        <button id="real">REAL</button>
    </div>
    <div class='accuracy' style='font-size: 18px;'>Accuracy: <span id="accuracy">0</span>%</div>
</div>


<!--HIDDEN DATA-->
<span id="hiddenArticles" style='display:none;'>
    <%= articles%>
</span>

<!--SCRIPTS-->
<script src="https://hammerjs.github.io/dist/hammer.min.js"></script>
<script src="js/slide.js"></script>
